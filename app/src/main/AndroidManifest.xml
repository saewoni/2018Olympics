<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.a2018olympics">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Sports"
            android:label="Sports"
            />
        <activity android:name=".Greetings"
            android:label="Greetings"
            />
        <activity android:name=".Courtesy"
            android:label="Courtesy"
            />
        <activity android:name=".Navigating"
            android:label="Navigating"
            />
        <activity android:name=".DineAndShop"
            android:label="DineAndShop"
            />
        <activity android:name=".Emergency"
            android:label="Emergency"
            />

    </application>

</manifest>